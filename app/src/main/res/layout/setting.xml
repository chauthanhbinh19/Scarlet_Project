<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<RelativeLayout android:id="@+id/settings"
		android:layout_width="match_parent"
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" >

			<View
				android:id="@+id/_bg__setting"
				android:layout_width="412dp"
				android:layout_height="800dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:background="@drawable/_bg__setting_shape" />


			<RelativeLayout android:id="@+id/group_1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>


				<RelativeLayout android:id="@+id/frame_2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>

					<View
						android:id="@+id/_bg__frame_2_ek1"
						android:layout_width="410dp"
						android:layout_height="99dp"
						android:layout_marginStart="0dp"
						android:layout_marginTop="0dp"
						android:background="@drawable/_bg__white" />

					<TextView android:id="@+id/settings_1"
						android:textStyle="bold"
						android:layout_gravity="center_horizontal|center"
						android:gravity="center"
						android:layout_centerHorizontal="true"
						android:textSize="20sp"
						android:textColor="@color/black"
						android:layout_marginLeft="100dp"
						android:layout_marginTop="30dp"
						android:layout_width="191dp"
						android:layout_height="33dp"
						android:text="@string/settings_string"
						 />

					<RelativeLayout android:id="@+id/back_btn"
						android:layout_marginTop="22dp"
						android:layout_marginStart="25dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						>


						<View   android:id="@+id/rectangle_3"
							android:background="@drawable/btn_burgundy"
							android:layout_marginStart="0dp"
							android:layout_marginTop="0dp"
							android:layout_width="45dp"
							android:layout_height="40dp" />


						<ImageView android:id="@+id/vector"
							android:src="@drawable/vector"
							android:contentDescription="vector"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true"
							android:layout_width="15dp"
							android:layout_height="15dp" />



					</RelativeLayout>

				</RelativeLayout>

			</RelativeLayout>



			<TextView android:id="@+id/setup_account"
				android:textStyle="bold"
				android:textSize="16sp"
				android:textColor="@color/_bg__group_1_ek1_color"
				android:layout_marginLeft="24dp"
				android:layout_marginTop="120dp"
				android:layout_width="157.6dp"
				android:layout_height="42.6dp"
				android:text="@string/setup_account_string"
				 />

			<TextView android:id="@+id/security"
				android:textStyle="bold"
				android:textSize="16sp"
				android:textColor="@color/black"
				android:layout_marginLeft="24dp"
				android:layout_marginTop="221dp"
				android:layout_width="157.6dp"
				android:layout_height="42.6dp"
				android:text="@string/security_string"
				 />


			<RelativeLayout android:id="@+id/group_35"
				android:layout_marginTop="171dp"
				android:layout_marginStart="17dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>




				<View
					android:id="@+id/rectangle_48"
					android:layout_width="350dp"
					android:layout_height="40dp"
					android:layout_marginStart="10dp"
					android:layout_marginTop="0dp"
					android:background="@drawable/rectangle_lightpink" />

				<TextView android:id="@+id/delete_account"
					android:textSize="16sp"
					android:textColor="@color/burgundy"
					android:layout_marginLeft="48dp"
					android:layout_marginTop="2dp"
					android:layout_width="226.6dp"
					android:layout_height="42.6dp"
					android:text="@string/delete_account_string"
					android:layout_centerVertical="true"
					android:gravity="center_vertical"
					 />

				<ImageView android:id="@+id/user_1"
					android:src="@drawable/user_1"
					android:contentDescription="user_1"
					 android:layout_marginStart="19dp"
					android:layout_marginTop="10dp"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_centerVertical="true"
					android:gravity="center_vertical"/>

			</RelativeLayout>


			<RelativeLayout android:id="@+id/group_36"
				android:layout_marginTop="269dp"
				android:layout_marginStart="17dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>


				<View   android:id="@+id/rectangle_48_ek1"
					android:background="@drawable/rectangle_lightpink"
					 android:layout_marginStart="10dp"
					android:layout_marginTop="0dp"
					android:layout_width="350dp"
					android:layout_height="40dp" />

				<TextView android:id="@+id/change_passsword"
					android:textSize="16sp"
					android:textColor="@color/burgundy"
					android:layout_marginLeft="48dp"
					android:layout_marginTop="2dp"
					android:layout_width="226.6dp"
					android:layout_height="42.6dp"
					android:layout_centerVertical="true"
					android:gravity="center_vertical"
					android:text="@string/change_passsword_string"
					 />

				<ImageView android:id="@+id/padlock_1"
					android:src="@drawable/padlock_1"
					android:contentDescription="padlock_1"
					 android:layout_marginStart="19dp"
					android:layout_marginTop="7dp"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_centerVertical="true"
					android:gravity="center_vertical"
					/>

				<ImageView android:id="@+id/right_arrow_12"
					android:src="@drawable/right_arrow_12"
					android:contentDescription="right_arrow_12"
					 android:layout_marginStart="320dp"
					android:layout_marginTop="13dp"
					android:layout_width="15dp"
					android:layout_height="15dp"
					android:layout_centerVertical="true"
					android:gravity="center_vertical"/>

			</RelativeLayout>

		</RelativeLayout>

	</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>