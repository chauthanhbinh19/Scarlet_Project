<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<ScrollView android:id="@+id/favourite_page"
		android:layout_width="match_parent"
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >

			<View
				android:id="@+id/_bg__favourite"
				android:layout_width="410dp"
				android:layout_height="800dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:background="@drawable/_bg__white" />

			<RelativeLayout android:id="@+id/frame_6"
				android:layout_marginTop="100dp"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				>

				<View
					android:id="@+id/_bg__frame_6_ek1"
					android:layout_width="410dp"
					android:layout_height="176dp"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:background="@drawable/_bg__frame_lightgray" />

				<View   android:id="@+id/rectangle_12"
					android:background="@drawable/rectangle_white_radius"
					 android:layout_marginStart="30dp"
					android:layout_marginTop="28dp"
					android:layout_width="120dp"
					android:layout_height="120dp" />

				<TextView android:id="@+id/black_coffee"
					android:textStyle="bold"
					android:layout_gravity="center_horizontal|center"
					android:gravity="center"
					android:textSize="20sp"
					android:textColor="@color/black"
					android:layout_marginLeft="154dp"
					android:layout_marginTop="28dp"
					android:layout_width="141dp"
					android:layout_height="33dp"
					android:text="@string/black_coffee_string"
					 />

				<TextView android:id="@+id/_20_000_"
					android:textStyle="bold"
					android:layout_gravity="center_horizontal|center"
					android:gravity="center"
					android:textSize="16sp"
					android:textColor="@color/black"
					android:layout_marginLeft="154dp"
					android:layout_marginTop="88dp"
					android:layout_width="141dp"
					android:layout_height="31.6dp"
					android:text="@string/_20_000__string"
					 />


				<RelativeLayout android:id="@+id/group_2"
					android:layout_marginTop="15dp"
					android:layout_marginStart="310dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>


					<View   android:id="@+id/ellipse_15"
						android:background="@drawable/ellipse_lightpink_small"
						 android:layout_marginStart="30dp"
						android:layout_marginTop="0dp"
						android:layout_width="35dp"
						android:layout_height="35dp" />

					<ImageView android:id="@+id/trash_bin_1"
						android:src="@drawable/trash_bin_1"
						android:contentDescription="trash_bin_1"
						 android:layout_marginStart="38dp"
						android:layout_marginTop="7dp"
						android:layout_width="20dp"
						android:layout_height="20dp" />

				</RelativeLayout>

				<ImageView android:id="@+id/dsc02313_optimized"
					android:src="@drawable/dsc02313_optimized"
					android:contentDescription="dsc02313_optimized"
					 android:layout_marginStart="30dp"
					android:layout_marginTop="28dp"
					android:layout_width="120dp"
					android:layout_height="120dp" />

				<ImageView android:id="@+id/line_1"
					android:src="@drawable/line_1"
					android:contentDescription="line_1"
					 android:layout_marginStart="25dp"
					android:layout_marginTop="175.48dp"
					android:layout_width="312dp"
					android:layout_height="3.19dp" />

			</RelativeLayout>



		</RelativeLayout>

	</ScrollView>


	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="80dp"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		android:background="@color/white"
		>
		<RelativeLayout android:id="@+id/back_btn"
			android:layout_marginTop="22dp"
			android:layout_marginStart="25dp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			>


			<View   android:id="@+id/rectangle_3"
				android:background="@drawable/btn_burgundy"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:layout_width="45dp"
				android:layout_height="40dp" />


			<ImageView android:id="@+id/vector"
				android:src="@drawable/vector"
				android:contentDescription="vector"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:layout_width="15dp"
				android:layout_height="15dp" />

		</RelativeLayout>

		<TextView android:id="@+id/favourite_1"
			android:textStyle="bold"
			android:layout_gravity="center_horizontal|center"
			android:gravity="center"
			android:layout_centerHorizontal="true"
			android:textSize="20sp"
			android:textColor="@color/black"
			android:layout_marginLeft="124dp"
			android:layout_marginTop="32dp"
			android:layout_width="113dp"
			android:layout_height="33dp"
			android:text="@string/favourite_string"
			/>
	</RelativeLayout>



	<RelativeLayout android:id="@+id/nav"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginTop="885dp"
		android:layout_marginStart="0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		>


		<ImageView android:id="@+id/ellipse_4"
			android:src="@drawable/navigation"
			android:contentDescription="ellipse_4"
			android:layout_marginStart="0dp"
			android:layout_marginTop="60dp"
			android:layout_width="412dp"
			android:layout_height="105dp"/>


		<RelativeLayout android:id="@+id/cart_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="40dp"
			android:clickable="true">



			<TextView
				android:id="@+id/cart"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="65dp"
				android:gravity="center"
				android:text="@string/cart_string"
				android:textColor="@color/burgundy"
				android:textSize="12sp" />


			<ImageView
				android:id="@+id/shopping_cart"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="39dp"
				android:contentDescription="shopping_cart"
				android:src="@drawable/shopping_cart" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/home_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="5dp"
			android:layout_marginTop="90dp"
			android:clickable="true">

			<ImageView android:id="@+id/home_ek1"
				android:src="@drawable/home_ek1"
				android:contentDescription="home_ek1"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="5dp"
				android:layout_width="30dp"
				android:layout_height="30dp" />

			<TextView android:id="@+id/home"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:textSize="12sp"
				android:textColor="@color/burgundy"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:layout_centerHorizontal="true"
				android:layout_width="54.2dp"
				android:layout_height="27.2dp"
				android:text="@string/home_string"
				/>
		</RelativeLayout>
		<RelativeLayout android:id="@+id/favourite_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="60dp"
			android:layout_marginTop="90dp"
			android:clickable="true">

			<TextView
				android:id="@+id/favourite"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:gravity="center"
				android:text="@string/favourite_string"
				android:textColor="@color/burgundy"
				android:textSize="12sp" />


			<ImageView
				android:id="@+id/heart"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="5dp"
				android:contentDescription="heart"
				android:src="@drawable/heart__1__1" />


		</RelativeLayout>
		<RelativeLayout android:id="@+id/deals_button"
			android:layout_marginStart="250dp"
			android:layout_marginTop="82dp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:clickable="true">


			<ImageView
				android:id="@+id/discount"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="10dp"
				android:contentDescription="discount"
				android:src="@drawable/discount" />

			<TextView
				android:id="@+id/deals"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="46dp"
				android:gravity="center"
				android:text="@string/deals_string"
				android:textColor="@color/burgundy"
				android:textSize="12sp" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/account_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="330dp"
			android:layout_marginTop="87dp"
			android:clickable="true">

			<ImageView android:id="@+id/user__1__1"
				android:src="@drawable/user"
				android:contentDescription="user__1__1"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="20px"
				android:layout_width="30dp"
				android:layout_height="30dp" />

			<TextView android:id="@+id/account_1"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:textSize="12sp"
				android:layout_centerHorizontal="true"
				android:textColor="@color/burgundy"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:text="@string/account_string"
				/>
		</RelativeLayout>

	</RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>