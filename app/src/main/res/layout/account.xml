<androidx.constraintlayout.widget.ConstraintLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<ScrollView android:id="@+id/account" 
		android:layout_width="match_parent"
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="match_parent"
			android:layout_height="wrap_content" >

			<View   android:id="@+id/frame"
				android:background="@drawable/_bg__white"
				android:layout_width="match_parent"
				android:layout_height="1025dp" />

			<View   android:id="@+id/rectangle_41"
				android:background="@drawable/rectangle_burgundy"
				 android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:layout_width="412dp"
				android:layout_height="215dp" />

			<View   android:id="@+id/ellipse_23"
				android:background="@drawable/ellipse_lightpink"
				 android:layout_marginStart="22dp"
				android:layout_marginTop="58dp"
				android:layout_width="100dp"
				android:layout_height="100dp" />

			<TextView android:id="@+id/user_name"
				android:textStyle="bold"
				android:textSize="20sp"
				android:textColor="@color/lightpink"
				android:layout_marginLeft="132dp"
				android:layout_marginTop="58dp"
				android:layout_width="166dp"
				android:layout_height="43dp"
				android:gravity="center_vertical"
				android:text="@string/user_zjh_string"
				 />

			<Button android:id="@+id/create_account_btn"
				android:textStyle="bold"
				android:textSize="16sp"
				android:textColor="@color/burgundy"
				android:layout_alignParentRight="true"
				android:layout_marginRight="20dp"
				android:layout_marginTop="58dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:paddingRight="10dp"
				android:paddingLeft="10dp"
				android:visibility="gone"
				android:text="Sign in"
				style="@style/MyButtonTheme"
				android:elevation="4dp"
				android:background="@drawable/rectangle_circle_white"
				/>

			<com.google.android.material.imageview.ShapeableImageView android:id="@+id/user_avatar"
				android:src="@drawable/fox"
				 android:layout_marginStart="32dp"
				android:layout_marginTop="68dp"
				android:scaleType="centerCrop"
				app:shapeAppearanceOverlay="@style/roundedImageView50"
				android:layout_width="80dp"
				android:layout_height="80dp" />


			<RelativeLayout android:id="@+id/frame_8"
				android:layout_marginTop="193dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__frame_8_ek1"
					android:background="@drawable/_bg_white_radius30"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="412dp"
					android:layout_height="717dp" />

				<TextView android:id="@+id/account_ek1"
					android:textStyle="bold"
					android:textSize="16sp"
					android:textColor="@color/black"
					android:layout_marginLeft="38dp"
					android:layout_marginTop="23dp"
					android:layout_width="115.8dp"
					android:layout_height="wrap_content"
					android:text="@string/account_ek1_string"
					 />

				<RelativeLayout android:id="@+id/profile_button"
					android:layout_marginTop="61dp"
					android:layout_centerHorizontal="true"
					android:background="@drawable/rectangle_lightpink_radius"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:layout_marginLeft="70dp"
					android:elevation="4dp"
					>

					<TextView android:id="@+id/profile_ek2"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/profile_ek2_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/edit_1"
						android:src="@drawable/edit_1"
						 android:layout_marginStart="17dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_3"
						android:src="@drawable/right_arrow_3"
						android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/settings_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/profile_button"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/settings_ek2"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/settings_ek2_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/setting_1"
						android:src="@drawable/setting_1"
						 android:layout_marginStart="17dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_4"
						android:src="@drawable/right_arrow_3"
						 android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/dashboard_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/settings_button"
					android:layout_centerHorizontal="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:visibility="gone"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Dashboard"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						/>

					<ImageView
						android:src="@drawable/dashboard"
						android:layout_marginStart="17dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView
						android:src="@drawable/right_arrow_3"
						android:layout_alignParentRight="true"
						android:layout_marginRight="20dp"
						android:layout_centerVertical="true"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<TextView android:id="@+id/order"
					android:textStyle="bold"
					android:textSize="16sp"
					android:layout_below="@id/dashboard_button"
					android:textColor="@color/black"
					android:layout_marginLeft="36dp"
					android:layout_marginTop="10dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="@string/order_string"
					/>

				<RelativeLayout android:id="@+id/order_activities_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/order"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/order_activities"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/order_activities_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/file_1"
						android:src="@drawable/file_1"
						 android:layout_marginStart="17dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView
						android:id="@+id/right_arrow_5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:src="@drawable/right_arrow_3" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/review_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/order_activities_button"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/review"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Send reviews"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						/>

					<ImageView android:id="@+id/reviewIcon"
						android:src="@drawable/review"
						android:layout_marginStart="17dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_15"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:src="@drawable/right_arrow_3" />

				</RelativeLayout>

				<TextView android:id="@+id/general_information"
					android:textStyle="bold"
					android:textSize="16sp"
					android:textColor="@color/black"
					android:layout_marginLeft="38dp"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/review_button"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="@string/general_information_string"
					/>

				<RelativeLayout android:id="@+id/policies_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/general_information"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/policies"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/policies_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/file__1__1"
						android:src="@drawable/file__1__1"
						android:layout_marginStart="15dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentRight="true"
						android:layout_marginRight="20dp"
						android:src="@drawable/right_arrow_3" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/loyalty_program_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/policies_button"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/loyalty_program"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/loyalty_program_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/file__1__2"
						android:src="@drawable/customer_loyalty"
						android:layout_marginStart="15dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_7"
						android:src="@drawable/right_arrow_3"
						android:layout_alignParentRight="true"
						android:layout_marginRight="20dp"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/app_version_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/loyalty_program_button"
					android:layout_centerHorizontal="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/about_app_version"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/about_app_version_string"
						android:layout_centerVertical="true"
						android:gravity="center_vertical"
						 />

					<ImageView android:id="@+id/information_button_1"
						android:src="@drawable/information_button_1"
						android:layout_marginStart="15dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView
						android:id="@+id/right_arrow_8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:layout_alignParentRight="true"
						android:src="@drawable/right_arrow_3" />

				</RelativeLayout>

				<TextView android:id="@+id/help_center"
					android:textStyle="bold"
					android:textSize="16sp"
					android:textColor="@color/black"
					android:layout_marginLeft="36dp"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/app_version_button"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="@string/help_center_string"
					/>

				<RelativeLayout android:id="@+id/faqs_button"
					android:layout_marginTop="10dp"
					android:visibility="gone"
					android:layout_below="@id/help_center"
					android:layout_centerHorizontal="true"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/faqs_ek1"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="@string/faqs_ek1_string"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						 />

					<ImageView android:id="@+id/chat_box_1"
						android:src="@drawable/chat_box_1"
						android:layout_marginStart="15dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_10"
						android:src="@drawable/right_arrow_3"
						android:layout_centerVertical="true"
						android:layout_alignParentRight="true"
						android:layout_marginRight="20dp"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<RelativeLayout android:id="@+id/feedback___support_button"
					android:layout_marginTop="10dp"
					android:layout_below="@+id/faqs_button"
					android:layout_centerHorizontal="true"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="12dp"
					android:elevation="4dp"
					android:layout_marginLeft="70dp"
					android:background="@drawable/rectangle_lightpink_radius"
					>

					<TextView android:id="@+id/feedback___support"
						android:textSize="16sp"
						android:textColor="@color/black"
						android:layout_marginLeft="60dp"
						android:layout_marginTop="11dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="center_vertical"
						android:layout_centerVertical="true"
						android:text="@string/feedback___support_string"
						 />

					<ImageView android:id="@+id/feedback_1"
						android:src="@drawable/feedback_1"
						android:layout_marginStart="15dp"
						android:layout_centerVertical="true"
						android:layout_width="25dp"
						android:layout_height="25dp" />

					<ImageView android:id="@+id/right_arrow_11"
						android:src="@drawable/right_arrow_3"
						android:layout_alignParentRight="true"
						android:layout_centerVertical="true"
						android:layout_marginRight="20dp"
						android:layout_width="15dp"
						android:layout_height="15dp" />

				</RelativeLayout>

				<Button android:id="@+id/sign_out_btn"
					android:background="@drawable/rectangle_burgundy_radius"
					android:layout_marginTop="30dp"
					android:textStyle="bold"
					android:layout_below="@+id/feedback___support_button"
					android:layout_width="match_parent"
					android:layout_height="48dp"
					android:layout_centerHorizontal="true"
					android:gravity="center"
					android:textSize="18sp"
					android:textColor="@color/white"
					android:layout_marginLeft="80dp"
					android:text="@string/sign_out_string"/>

			</RelativeLayout>

		</RelativeLayout>

	</ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>