<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent" android:layout_height="wrap_content" >

	<ScrollView android:id="@+id/app_version" 
		android:layout_width="wrap_content" 
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >

			<View   android:id="@+id/_bg__app_version"
				android:background="@drawable/_bg__lightpink"
				android:layout_marginTop="0dp"
				android:layout_width="410dp"
				android:layout_height="800dp" />


			<RelativeLayout android:id="@+id/frame_1"
				android:layout_marginTop="224dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__frame_1_ek1"
					android:background="@drawable/_bg__lightpink"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="410dp"
					android:layout_height="216dp" />

				<View   android:id="@+id/ellipse_1"
					android:background="@drawable/ellipse_burgundy"
