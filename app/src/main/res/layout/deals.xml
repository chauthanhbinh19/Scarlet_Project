<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	xmlns:app="http://schemas.android.com/apk/res-auto">
	<ScrollView android:id="@+id/deals_page"
		android:layout_width="match_parent"
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >

			<View   android:id="@+id/_bg__deals"
				android:background="@drawable/_bg__white"
				 android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:layout_width="410dp"
				android:layout_height="800dp" />


					<View   android:id="@+id/rectangle_1"
						android:background="@drawable/rectangle_lightpink"
						 android:layout_marginStart="0dp"
						android:layout_marginTop="84dp"
						android:layout_width="410dp"
						android:layout_height="50dp" />

					<TextView android:id="@+id/deals_1"
						android:textStyle="bold"
						android:layout_gravity="center_horizontal|center"
						android:gravity="center"
						android:textSize="20sp"
						android:textColor="@color/black"
						android:layout_marginLeft="124dp"
						android:layout_marginTop="32dp"
						android:layout_width="113dp"
						android:layout_height="33dp"
						android:text="@string/deals_string"
						 />

					<TextView android:id="@+id/uit"
						android:textStyle="bold"
						android:layout_gravity="center_horizontal|center"
						android:gravity="center"
						android:textSize="14sp"
						android:textColor="@color/black"
						android:layout_marginLeft="125dp"
						android:layout_marginTop="96dp"
						android:layout_width="112.4dp"
						android:layout_height="31.4dp"
						android:text="@string/uit_string"
						 />


					<RelativeLayout android:id="@+id/back_btn"
						android:layout_marginTop="22dp"
						android:layout_marginStart="25dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						>


						<View   android:id="@+id/rectangle_3"
							android:background="@drawable/btn_burgundy"
							android:layout_marginStart="0dp"
							android:layout_marginTop="0dp"
							android:layout_width="45dp"
							android:layout_height="40dp" />


						<ImageView android:id="@+id/vector"
							android:src="@drawable/vector"
							android:contentDescription="vector"
							android:layout_centerHorizontal="true"
							android:layout_centerVertical="true"
							android:layout_width="15dp"
							android:layout_height="15dp" />



					</RelativeLayout>

					<ImageView android:id="@+id/vector_ek1"
						android:src="@drawable/vector_ek1"
						android:contentDescription="vector_ek1"
						 android:layout_marginStart="34dp"
						android:layout_marginTop="96dp"
						android:layout_width="20dp"
						android:layout_height="20dp" />


			<RelativeLayout android:id="@+id/frame_6"
				android:layout_marginTop="184dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<View   android:id="@+id/_bg__frame_6_ek1"
					android:background="@drawable/_bg__frame_lightgray"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="410dp"
					android:layout_height="176dp" />

				<View   android:id="@+id/rectangle_12"
					android:background="@drawable/rectangle_white_radius"
					 android:layout_marginStart="30dp"
					android:layout_marginTop="28dp"
					android:layout_width="120dp"
					android:layout_height="120dp" />

				<TextView android:id="@+id/black_friday"
					android:textStyle="bold"
					android:textSize="18sp"
					android:textColor="@color/black"
					android:layout_marginLeft="163dp"
					android:layout_marginTop="28dp"
					android:layout_width="149.8dp"
					android:layout_height="32.8dp"
					android:text="@string/black_friday_string"
					 />

				<TextView android:id="@+id/discount_50__for_every_product"
					android:textStyle="bold"
					android:textSize="12sp"
					android:textColor="@color/black"
					android:layout_marginLeft="163dp"
					android:layout_marginTop="61dp"
					android:layout_width="209.2dp"
					android:layout_height="30.2dp"
					android:text="@string/discount_50__for_every_product_string"
					 />

				<ImageView android:id="@+id/dsc02313_optimized"
					android:src="@drawable/dsc02313_optimized"
					android:contentDescription="dsc02313_optimized"
					 android:layout_marginStart="30dp"
					android:layout_marginTop="28dp"
					android:layout_width="120dp"
					android:layout_height="120dp" />

				<ImageView android:id="@+id/line_1"
					android:src="@drawable/line_1"
					android:contentDescription="line_1"
					 android:layout_marginStart="25dp"
					android:layout_marginTop="175.48dp"
					android:layout_width="312dp"
					android:layout_height="0.52dp" />

				<View   android:id="@+id/rectangle_39"
					android:background="@drawable/rectangle_burgundy_radius"
					 android:layout_marginStart="254dp"
					android:layout_marginTop="113dp"
					android:layout_width="84dp"
					android:layout_height="35dp" />

				<TextView android:id="@+id/order_now"
					android:textStyle="bold"
					android:layout_gravity="center_horizontal|center"
					android:gravity="center"
					android:textSize="12sp"
					android:textColor="@color/white"
					android:layout_marginLeft="257dp"
					android:layout_marginTop="120dp"
					android:layout_width="80.2dp"
					android:layout_height="25.2dp"
					android:text="@string/order_now_string"
					 />

			</RelativeLayout>




		</RelativeLayout>

	</ScrollView>


	<RelativeLayout android:id="@+id/nav"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		android:layout_marginTop="885dp"
		android:layout_marginStart="0dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		>


		<ImageView android:id="@+id/ellipse_4"
			android:src="@drawable/navigation"
			android:contentDescription="ellipse_4"
			android:layout_marginStart="0dp"
			android:layout_marginTop="60dp"
			android:layout_width="412dp"
			android:layout_height="105dp"/>


		<RelativeLayout android:id="@+id/cart_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_centerHorizontal="true"
			android:layout_marginTop="40dp"
			android:clickable="true">



			<TextView
				android:id="@+id/cart"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="65dp"
				android:gravity="center"
				android:text="@string/cart_string"
				android:textColor="@color/faqs_color"
				android:textSize="12sp" />


			<ImageView
				android:id="@+id/shopping_cart"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="39dp"
				android:contentDescription="shopping_cart"
				android:src="@drawable/shopping_cart" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/home_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="5dp"
			android:layout_marginTop="90dp"
			android:clickable="true">

			<ImageView android:id="@+id/home_ek1"
				android:src="@drawable/home_ek1"
				android:contentDescription="home_ek1"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="5dp"
				android:layout_width="30dp"
				android:layout_height="30dp" />

			<TextView android:id="@+id/home"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:textSize="12sp"
				android:textColor="@color/faqs_color"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:layout_centerHorizontal="true"
				android:layout_width="54.2dp"
				android:layout_height="27.2dp"
				android:text="@string/home_string"
				/>
		</RelativeLayout>
		<RelativeLayout android:id="@+id/favourite_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="60dp"
			android:layout_marginTop="90dp"
			android:clickable="true">

			<TextView
				android:id="@+id/favourite"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:layout_gravity="center_horizontal|center"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:gravity="center"
				android:text="@string/favourite_string"
				android:textColor="@color/faqs_color"
				android:textSize="12sp" />

			<ImageView
				android:id="@+id/heart"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="5dp"
				android:contentDescription="heart"
				android:src="@drawable/heart" />


		</RelativeLayout>
		<RelativeLayout android:id="@+id/deals_button"
			android:layout_marginStart="250dp"
			android:layout_marginTop="82dp"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:clickable="true">


			<ImageView android:id="@+id/discount"
				android:src="@drawable/discount_1"
				android:contentDescription="discount"
				android:layout_centerHorizontal="true"
				android:layout_marginStart="40dp"
				android:layout_marginTop="10dp"
				android:layout_width="40dp"
				android:layout_height="40dp" />

			<TextView android:id="@+id/deals"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:textSize="12sp"
				android:textColor="@color/faqs_color"
				android:layout_centerHorizontal="true"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="46dp"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:text="@string/deals_string"
				/>
		</RelativeLayout>
		<RelativeLayout android:id="@+id/account_button"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="330dp"
			android:layout_marginTop="87dp"
			android:clickable="true">

			<ImageView android:id="@+id/user__1__1"
				android:src="@drawable/user"
				android:contentDescription="user__1__1"
				android:layout_centerHorizontal="true"
				android:layout_marginTop="20px"
				android:layout_width="30dp"
				android:layout_height="30dp" />

			<TextView android:id="@+id/account_1"
				android:layout_gravity="center_horizontal|center"
				android:gravity="center"
				android:textSize="12sp"
				android:layout_centerHorizontal="true"
				android:textColor="@color/faqs_color"
				android:layout_marginLeft="0dp"
				android:layout_marginTop="40dp"
				android:layout_width="91.2dp"
				android:layout_height="27.2dp"
				android:text="@string/account_string"
				/>
		</RelativeLayout>

	</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
